{
  "name": "maadhattah/wp-gistpen",
  "description": "A self-hosted alternative to putting your code snippets on Gist.",
  "license": "GPL-2.0+",
  "version": "0.4.0",
  "homepage": "http://jamesdigioia.com/wp-gistpen/",
  "authors": [
    {
      "name": "James DiGioia",
      "email": "jamesorodig@gmail.com"
    }
  ],
  "repositories": [
  {
    "type": "package",
    "package": {
      "name": "webdevstudios/cmb",
      "type": "private",
      "version": "1.2.0",
      "dist": {
        "type": "zip",
        "url": "https://github.com/WebDevStudios/Custom-Metaboxes-and-Fields-for-WordPress/archive/v1.2.0.zip"
      },
      "require": {
        "composer/installers": "v1.0.12"
      }
    }
  }],
  "require": {
    "php": ">=5.3.0",
      "composer/installers": "v1.0.12",
      "webdevstudios/cmb": "1.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.3.*",
    "mockery/mockery": "0.9.2"
  },
  "autoload": {
    "psr-4": {
      "WP_Gistpen\\": "app/"
    }
  },
  "config": {
    "vendor-dir": "lib"
  }
}
