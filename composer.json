{
  "name": "maadhattah/wp-gistpen",
  "description": "A self-hosted alternative to putting your code snippets on Gist.",
  "license": "GPL-2.0+",
  "version": "0.4.0",
  "homepage": "http://jamesdigioia.com/wp-gistpen/",
  "authors": [
    {
      "name": "James DiGioia",
      "email": "jamesorodig@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
      "composer/installers": "v1.0.12",
      "webdevstudios/cmb2": "dev-master#d29bdc6d55a5f481fde475cc307b4719311590c6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.3.*",
    "mockery/mockery": "0.9.2"
  },
  "autoload": {
    "psr-4": {
      "WP_Gistpen\\": "app/"
    },
    "files": ["lib/webdevstudios/cmb2/init.php"]
  },
  "config": {
    "vendor-dir": "lib"
  },
  "extra": {
    "installer-paths": {
      "lib/{$vendor}/{$name}/": ["webdevstudios/cmb2"]
    }
  }
}
