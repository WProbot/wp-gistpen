<pre class="gistpen line-numbers"
        {{#if blob.highlight}}data-line="{{blob.highlight}}"{{/if}}
        {{#if blob.edit_url}}data-edit-url="{{{blob.edit_url}}}"{{/if}}
        data-filename="{{blob.filename}}"><code
        class="language-{{prism_slug blob.language.slug}}">{{blob.code}}</code></pre>
