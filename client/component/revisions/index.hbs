<div data-brk-container="revisions" class="wpgp-revisions">
    <div class="wpgp-revisions-row">
        <a href="{{link "wpgp_route" "editor"}}" type="button"
                class="dashicons-before wpgp-button wpgp-button-add">
            Return to Editor
        </a>
    </div>

    {{#each revisions as |revision|}}
        <div class="wpgp-revisions-row">
            <ul>
                <li>{{revision.date}}</li>
            </ul>
        </div>
    {{/each}}
</div>
