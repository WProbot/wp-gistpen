{
  "plugins": [
    "syntax-flow",
    "transform-flow-strip-types"
  ],
  "presets": [
    ["brookjs", { "loose": false, "modules": false }]
  ],
  "env": {
    "production": {
      "plugins": [
        ["tcomb", {
          "skipAsserts": true,
          "globals": [
            {
              "Disposer": true
            },
            {
              "ProxyEvent": true
            },
            {
              "Node": true
            }
          ]
        }]
      ]
    },
    "development": {
      "plugins": [
        ["tcomb", {
          "globals": [
            {
              "Disposer": true
            },
            {
              "ProxyEvent": true
            },
            {
              "Node": true
            }
          ]
        }],
      ]
    }
  }
}
