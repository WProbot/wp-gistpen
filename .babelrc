{
  "plugins": [
    "ramda",
    "transform-array-from",
    "transform-object-assign",
    "transform-object-rest-spread"
  ],
  "presets": [
    ["es2015", { "loose": false, "modules": false }]
  ],
  "env": {
    "test": {
      "presets": [
        ["es2015", { "loose": false, "modules": "commonjs" }]
      ]
    }
  }
}
