    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-brookjs": "^0.11.0",
    "brookjs-cli": "^0.11.2",
    "brookjs-desalinate": "^0.11.2",
    "chai": "^4.0.2",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.4",
    "dredd": "^4.9.2",
    "eslint": "^4.14.0",
    "eslint-config-valtech": "^1.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "flow-bin": "^0.61.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "gulp": "3.9",
    "gulp-sort": "^2.0.0",
    "gulp-util": "^3.0.7",
