    "handlebars": "^4.0.5",
    "handlebars-loader": "^1.1.4",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "marked": "^0.3.9",
    "node-notifier": "^5.0.0",
    "node-sass": "^4.5.1",
    "sass-loader": "^6.0.3",
    "shelljs": "^0.7.8",
    "simulant": "^0.2.2",
    "sinon": "^4.1.3",
    "sinon-chai": "^2.9.0",
    "style-loader": "^0.19.1",
    "stylelint-webpack-plugin": "^0.7.0",
    "symbol-observable": "^1.1.0",
