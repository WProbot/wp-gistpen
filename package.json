{
  "name": "wp-gistpen",
  "description": "A self-hosted alternative to putting your code snippets on Gist.",
  "author": {
    "name": "James DiGioia",
    "email": "jamesorodig@gmail.com",
    "twitter": "@JamesDiGioia"
  },
  "homepage": "http://www.jamesdigioia.com/wp-gistpen/",
  "license": "GPL-2.0+",
  "scripts": {
    "test": "npm run test:typecheck && npm run test:lint && npm run test:unit",
    "test:typecheck": "exit 0",
    "test:lint": "eslint ./client",
    "test:unit": "beaver test unit --env production",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "beaver build --env development",
    "build:prod": "beaver build --env production",
    "dev:app": "beaver dev app --env development",
    "dev:tdd": "beaver dev tdd --env development",
    "dev:storybook": "start-storybook -s ./assets/js -p 9001 -c .storybook",
    "gen": "npm run gen:coc && npm run gen:pot",
    "gen:coc": "weallbehave -o .",
    "gen:pot": "wp-pot --bug-report http://github.com/intraxia/wp-gistpen --domain wp-gistpen --package wp-gistpen --src 'app/**/*.php' --relative-to . --dest-file languages/wp-gistpen.pot"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-flow": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/react": "^4.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-ramda": "^2.0.0",
    "babel-preset-brookjs": "1.0.0-alpha.5",
    "brookjs-cli": "1.0.0-alpha.7",
    "brookjs-desalinate": "1.0.0-alpha.7",
    "chai": "^4.0.2",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-valtech": "^1.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-react": "^7.6.1",
    "esm": "^3.0.10",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.0",
    "shelljs": "^0.8.1",
    "sinon": "^7.2.6",
    "sinon-chai": "^3.0.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.2.1",
    "stylelint-webpack-plugin": "^0.10.1",
    "symbol-observable": "^1.2.0",
    "weallbehave": "^1.2.0",
    "webpack-notifier": "^1.5.1",
    "wp-pot-cli": "^1.0.3"
  },
  "dependencies": {
    "bourbon": "^5.0.0",
    "bourbon-neat": "^1.8.0",
    "brookjs": "1.0.0-alpha.6",
    "brookjs-silt": "1.0.0-alpha.7",
    "classnames": "^2.2.5",
    "clipboard": "^2.0.0",
    "core-js": "^2.5.3",
    "jss": "^9.6.0",
    "jss-nested": "^6.0.1",
    "kefir": "^3.8.1",
    "mutationobserver-shim": "^0.3.2",
    "prism-themes": "^1.0.0",
    "prismjs": "1.15.0",
    "ramda": "^0.26.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-jss": "^8.2.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "sheet-router": "^4.0.0-0",
    "sprintf-js": "^1.1.1",
    "util": "^0.10.0",
    "whatwg-fetch": "^2.0.2"
  },
  "esm": {
    "mode": "auto",
    "cjs": true
  }
}
